{
  "name": "fastify-auth0-login",
  "version": "0.0.1",
  "description": "Fastify plugin easily adding login feature via Auth0's Authorization Code Flow",
  "author": {
    "name": "Hyeseong Kim",
    "email": "hey@hyeseong.kim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cometkim/fastify-auth0-login.git"
  },
  "keywords": [
    "fastify",
    "fastify-plugin",
    "auth",
    "authentication",
    "authorization",
    "auth0"
  ],
  "main": "./lib/index.cjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepack": "yarn build --clean",
    "build": "nanobundle build --platform node"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "@fastify/cookie": "^8.0.0"
  },
  "dependencies": {
    "fast-jwt": "^2.2.0",
    "fastify-plugin": "^4.5.0"
  },
  "devDependencies": {
    "@fastify/cookie": "^8.3.0",
    "@types/node": "^18.14.2",
    "fastify": "^4.13.0",
    "nanobundle": "^1.5.0",
    "typescript": "^4.9.5"
  },
  "packageManager": "yarn@3.4.1"
}
